{
    "secret": "YOUR_SECRET_HERE",
    "configId": "YOUR_CLIENT_ID_HERE",
    "myId": "YOUR_USER_ID_HERE",
    
    "certPath": "../conf/public.pem",
    "keyPath": "../conf/private.pem",

    "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "auth_title",
        "Access-Control-Max-age": 600
    },

    "port": 48091,

    "initBoard": "/initBoard",

    "authTitle": "extension-jwt",

    "extensionURL": "https://api.twitch.tv/extension/YOUR_CLIENT_ID_HERE/",
    "apiURL": "https://api.twitch.tv/helix/",
    "alg": "YOUR_PREFERRED_ALGORITHM_HERE",
    "jwtHeader": {
        "alg": "YOUR_PREFERRED_ALGORITHM_HERE",
        "typ": "jwt"
    },
    "expirationTime": 600,
    "payloadRole": "external", 

    "limit": 100,
    "SQLHost": "localhost",
    "SQLUser": "root",
    "SQLPassword": "YOUR_PASSWORD_HERE",
    "database": "YOUR_DATABASE_HERE"

}
